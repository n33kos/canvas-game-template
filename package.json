{
  "name": "newGame",
  "version": "0.0.1",
  "private": true,
  "description": "A template for newGame",
  "main": "dist/js/newGame.min.js",
  "scripts": {
    "build": "webpack && npm run styles",
    "styles": "npm run scss && npm run cssprefix",
    "scss": "sass --no-source-map ./src/scss/styles.scss ./dist/styles/styles_temp.css",
    "cssprefix": "npx postcss ./dist/styles/styles_temp.css > ./dist/styles/styles.css && rm ./dist/styles/styles_temp.css",
    "serve": "http-server"
  },
  "author": "n33kos",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "http-server": "^0.11.1",
    "postcss-cli": "^6.0.0",
    "sass": "^1.15.2",
    "throttle-debounce": "^2.0.1",
    "uglifyjs-webpack-plugin": "^2.1.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3"
  }
}
